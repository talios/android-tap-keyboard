<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="10%p"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    >

    <Row android:keyHeight="@dimen/key_height">
        <Key android:codes="46" android:keyLabel="." android:keyWidth="50%p" android:keyEdgeFlags="left"/>
		<Key android:codes="45" android:keyLabel="-" android:keyWidth="50%p" android:keyEdgeFlags="right"/>
    </Row>
    
    <Row android:keyHeight="@dimen/key_height2">
        <Key android:codes="-3" android:keyIcon="@drawable/sym_keyboard_done" 
                android:keyWidth="20%p" android:keyEdgeFlags="left"/>
        <Key android:codes="-99" android:keyIcon="@drawable/arrow_non" 
                android:keyWidth="30%p" android:isRepeatable="true"/>
        <Key android:codes="32" android:keyLabel="Space" 
                android:keyWidth="30%p" android:isRepeatable="true"/>
        <Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete" 
                android:keyWidth="20%p" android:keyEdgeFlags="right"
                android:isRepeatable="true"/>
    </Row>
</Keyboard>
    